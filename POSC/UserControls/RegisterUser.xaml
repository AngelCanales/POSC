<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:POSC.UserControls"
             xmlns:ModelsView="clr-namespace:POSC.ModelsView" x:Class="POSC.UserControls.RegisterUser"
             mc:Ignorable="d" Height="408.58" Width="301.268" >
    <Grid Margin="0,0,10,51" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="133,36,0,0" TextWrapping="Wrap" Text="{Binding Name}" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="133,77,0,0" TextWrapping="Wrap" Text="{Binding Email}" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Full Name:" HorizontalAlignment="Left" Margin="62,33,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Email:" HorizontalAlignment="Left" Margin="89,74,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox2" Text="{Binding RTN}" HorizontalAlignment="Left" Height="23" Margin="133,122,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label4" Content="RTN:" HorizontalAlignment="Left" Margin="89,119,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="133,153,0,0" TextWrapping="Wrap" Text="{Binding PhoneNumber1}" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.496,1.108"/>
        <Label x:Name="label5" Content="Phone:" HorizontalAlignment="Left" Margin="75,153,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Height="23" Margin="133,190,0,0" TextWrapping="Wrap" Text="{Binding PhoneNumber2}" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.496,1.108"/>
        <Label x:Name="label6" Content="Contact Phone :" HorizontalAlignment="Left" Margin="34,187,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox5" HorizontalAlignment="Left" Height="23" Margin="133,231,0,0" TextWrapping="Wrap" Text="{Binding ContactName}" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.496,1.108"/>
        <Label x:Name="label7" Content="Name Contact:" HorizontalAlignment="Left" Margin="36,228,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" 
                  ItemsSource="{Binding EmployeesType}"
                  SelectedValue="{Binding EmployeesTypeID}"  
                  DisplayMemberPath="Name" 
                  SelectedValuePath="Id" 
                  HorizontalAlignment="Left" Margin="133,274,0,0" VerticalAlignment="Top" Width="120"/>

        <ComboBox x:Name="comboBox1" ItemsSource="{Binding Store}"
                  SelectedValue="{Binding StoreId}"  
                  DisplayMemberPath="Name" 
                  SelectedValuePath="Id" 
                  HorizontalAlignment="Left" Margin="133,313,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label3" Content="EmployeesType:" HorizontalAlignment="Left" Margin="34,274,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label8" Content="Store:" HorizontalAlignment="Left" Margin="83,309,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
