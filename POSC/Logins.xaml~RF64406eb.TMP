<Window x:Class="POSC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POSC"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="171,128,0,0" VerticalAlignment="Top" Width="120" Click="button_Click" Height="28"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="171,53,0,0" TextWrapping="Wrap" Text="{Binding Path=Email}" ToolTip="Password" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="{Binding Path=Password }" HorizontalAlignment="Left" Margin="115,50,0,0" VerticalAlignment="Top" Height="22" Width="38"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="115,77,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.085,-0.112"/>
        <PasswordBox x:Name="passwordBox" Tag="{Binding Path=Password }" HorizontalAlignment="Left" Margin="170,85,0,0" VerticalAlignment="Top" Width="121" Height="23"/>

    </Grid>
</Window>
